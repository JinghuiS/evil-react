{
  "name": "vite-react-di",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@wendellhu/redi": "^0.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6",
    "reset-css": "^5.0.1",
    "rxjs": "^7.5.5",
    "vite-plugin-windicss": "^1.8.3",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^1.0.7",
    "less": "^4.1.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "sass": "^1.49.11",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
